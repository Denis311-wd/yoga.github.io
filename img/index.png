        function wo_rand(min, max)
        {
            return Math.floor(Math.random() * (max - min + 1)) + min;
        }
        function wo_elem() {
            var ul_elem_0 = AZJQ("ul[lvl=0]");
            var li_elem_0 = AZJQ("ul[lvl=0]").children("LI[index=" + AZJQ("ul[lvl=0]").attr("show") + "]");
            var ul_elem_1 = li_elem_0.children(".wo_under_wrap[index=" + ul_elem_0.attr("show") + "]").children("ul");
            var li_elem_1 = ul_elem_1.children("LI[index=" + ul_elem_1.attr("show") + "]");
            var ul_elem_2 = li_elem_1.children(".wo_under_wrap[index=" + ul_elem_1.attr("show") + "]").children("ul");
            var li_elem_2 = ul_elem_2.children("LI[index=" + ul_elem_2.attr("show") + "]");
            var ul_elem_3 = li_elem_2.children(".wo_under_wrap[index=" + ul_elem_2.attr("show") + "]").children("ul");
            var li_elem_3 = ul_elem_3.children("LI[index=" + ul_elem_3.attr("show") + "]");
            var ul_elem_4 = li_elem_3.children(".wo_under_wrap[index=" + ul_elem_3.attr("show") + "]").children("ul");
            var li_elem_4 = ul_elem_4.children("LI[index=" + ul_elem_4.attr("show") + "]");
            var ul_elem_5 = li_elem_4.children(".wo_under_wrap[index=" + ul_elem_4.attr("show") + "]").children("ul");
            var li_elem_5 = ul_elem_5.children("LI[index=" + ul_elem_5.attr("show") + "]");
            return {
                ul0: ul_elem_0,
                ul1: ul_elem_1,
                ul2: ul_elem_2,
                ul3: ul_elem_3,
                ul4: ul_elem_4,
                ul5: ul_elem_5,
                li0: li_elem_0,
                li1: li_elem_1,
                li2: li_elem_2,
                li3: li_elem_3,
                li4: li_elem_4,
                li5: li_elem_5
            };
        }
        function wo_height_control() {
            var wrap_height = 0;
            wrap_height = wo_elem().ul0.height();
            if (wrap_height < wo_elem().ul1.height()) {
                wrap_height = wo_elem().ul1.height();
            }
            if (wrap_height < wo_elem().ul2.height()) {
                wrap_height = wo_elem().ul2.height();
            }
            if (wrap_height < wo_elem().ul3.height()) {
                wrap_height = wo_elem().ul3.height();
            }
            if (wrap_height < wo_elem().ul4.height()) {
                wrap_height = wo_elem().ul4.height();
            }
            if (wrap_height < wo_elem().ul5.height()) {
                wrap_height = wo_elem().ul5.height();
            }
            //console.log("wrap_height: " + wrap_height);
            return wrap_height + 20;
        }
        function wo_dropdown_sizing() {
            var webkitFix = 0;
            if(AZJQ.browser.webkit) {
                webkitFix = 1;
            }
            var lvl = 1;
            var element_height = AZJQ(".wo_dropdown LI[lvl=1]:first-child").height();
            //console.log("AZJQ('.wo_dropdown LI[lvl=1]').height() = " + element_height);
            AZJQ(".wo_dropdown .wo_under_wrap[lvl=1]").each(function() {
                var top_position = (AZJQ(this).attr("index") - 1) * element_height + 10;
                AZJQ(this).css({"top": "-" + top_position + "px"});
                AZJQ(".wo_under_wrap[lvl=2]", this).each(function() {
                    var lvl = 2;
                    var top_position = Math.floor((AZJQ(this).attr("index") - 1) * (element_height- webkitFix))+10;
                    AZJQ(this).css({"top": "-" + top_position + "px"});
                    var sss = 0;

                    console.log("-" + top_position + "px");
                    AZJQ(".wo_under_wrap[lvl=3]", this).each(function() {
                        var lvl = 3;
                        sss++;
                        var top_position = ((AZJQ(this).attr("index") - 1) * (element_height - webkitFix)) + 10;
                        AZJQ(this).css({"top": "-" + top_position + "px"});
                        AZJQ(".wo_under_wrap[lvl=4]", this).each(function() {
                            var lvl = 4;
                            sss++;
                            var top_position = ((AZJQ(this).attr("index") - 1) * (element_height - webkitFix)) + 10;
                            AZJQ(this).css({"top": "-" + top_position + "px"});
                            AZJQ(".wo_under_wrap[lvl=5]", this).each(function() {
                                var lvl = 5;
                                sss++;
                                var top_position = ((AZJQ(this).attr("index") - 1) * (element_height - webkitFix)) + 10;
                                AZJQ(this).css({"top": "-" + top_position + "px"});
                            });
                        //console.log("-" + top_position + "px");
                        });
                    });
                });
            });
        }

        AZJQ(document).ready(function() {
            AZJQ('.main-lever').height($(window).height() - 83);
            var superHeight = AZJQ('.main-lever').height();
            $('.lever').height(superHeight);
            $('.bnav .lever ul').height(superHeight + 3);
            $('.lever_prz').height(superHeight + 20);

            AZJQ(".wo_dropdown").mouseenter(function() {
                //console.log("jq work, well done");
            });
            AZJQ(".wo_wrap").css({"display": "block"});
            AZJQ(".wo_dropdown").css({"padding": "10px 20px"});
            AZJQ(".wo_one_button_block").mouseenter(function() {
                AZJQ(".wo_wrap").css({"display": "block"});
            });
            AZJQ(".wo_one_button_block").mouseleave(function() {
                AZJQ(".wo_wrap").css({"display": "none"});
            });
            wo_dropdown_sizing();
            var dropdown_w = AZJQ(".wo_dropdown").innerWidth();
            var dropdown_h = AZJQ(".wo_dropdown").innerHeight();
            AZJQ(".wo_wrap").css({"width": dropdown_w, "height": dropdown_h});


            AZJQ(".wo_dropdown").mouseleave(function() {
                AZJQ(".wo_wrap").height(AZJQ(".wo_dropdown").innerHeight());
                AZJQ(".wo_wrap").width(AZJQ(".wo_dropdown").innerWidth());
            });
            AZJQ(".wo_dropdown LI").mouseleave(function() {
                var attr_lvl = (AZJQ(this).parent().attr("lvl") * 1);
                if (AZJQ(this).attr("class") == "dir") {
                    //console.log(" DIR ");
                    AZJQ(".wo_wrap").css({"width": (dropdown_w + 30) * (attr_lvl + 1) - 30});
                    //console.log("wo_wrap width: " + (dropdown_w + 30) * attr_lvl - 30);
                }
                AZJQ(this).children(".wo_under_wrap").css({"display": "none"});
            });
            AZJQ(".wo_dropdown li").mouseenter(function() {

                //setTimeout

                //console.log(".wo_dropdown li actived");
                AZJQ(this).parent().attr("show", AZJQ(this).attr("index"));
                var attr_lvl = (AZJQ(this).parent().attr("lvl") * 1) + 1;
                if (AZJQ(this).attr("class") == "dir") {
                    //console.log(" DIR ");
                    AZJQ(".wo_wrap").css({"width": (dropdown_w + 30) * (attr_lvl + 1) - 30});

                    //console.log("wo_wrap width: " + (dropdown_w + 30) * attr_lvl - 30);
                }

                /*
                 var elems = AZJQ(".wo_dropdown").children().length;
                 if (attr_lvl < elems) {

                 } */

                AZJQ(this).children(".wo_under_wrap").css({"display": "block"});

                var wrap_height = 0;
                wrap_height = wo_elem().ul0.height();
                if (wrap_height < wo_elem().ul1.height()) {
                    wrap_height = wo_elem().ul1.height();
                }
                if (wrap_height < wo_elem().ul2.height()) {
                    wrap_height = wo_elem().ul2.height();
                }
                if (wrap_height < wo_elem().ul3.height()) {
                    wrap_height = wo_elem().ul3.height();
                }
                if (wrap_height < wo_elem().ul4.height()) {
                    wrap_height = wo_elem().ul4.height();
                }
                if (wrap_height < wo_elem().ul5.height()) {
                    wrap_height = wo_elem().ul5.height();
                }
                AZJQ(this).children().children().css({"height": wrap_height});
                //console.log("CHILDREN HEIGHT: " + dropdown_h + " === " + AZJQ(this).children().children().attr("lvl"));
                //console.log("wrap_height: " + wrap_height);

                AZJQ(".wo_wrap").height(wrap_height + 20);



            });

            AZJQ(".wo_wrap").css({"display": "none"});

            AZJQ('#left-e-menu-popup').click(function(){
                AZJQ('.main-lever').toggle();
            });
        });
    
</script>
